# API Configuration
REACT_APP_API_URL=http://localhost:5000

# Application Environment
REACT_APP_NODE_ENV=development

# Company Information
REACT_APP_COMPANY_NAME=Eagles Events
REACT_APP_COMPANY_EMAIL=info@eaglesevents.co.za
REACT_APP_COMPANY_PHONE=+27-XX-XXX-XXXX

# Social Media Links (optional)
REACT_APP_FACEBOOK_URL=https://facebook.com/eaglesevents
REACT_APP_INSTAGRAM_URL=https://instagram.com/eaglesevents
REACT_APP_TWITTER_URL=https://twitter.com/eaglesevents

# Google Analytics (optional)
REACT_APP_GA_TRACKING_ID=G-XXXXXXXXXX

